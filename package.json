    "run-sequence": "^1.1.5",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "eslintConfig": {
    "extends": "xo-space/esnext",
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "chrome": true,
      "$": true,
      "_": true,
      "jQuery": true
    },
    "rules": {
      "babel/new-cap": 0,
      "curly": [
        "error",
        "multi",
        "consistent"
      ],
      "max-len": [
        "error",
        {
          "code": 100,
          "ignoreUrls": true
        }
      ]
    }
  },
  "dependencies": {
    "jquery": "^3.1.0",
    "wolfy87-eventemitter": "^5.1.0"
